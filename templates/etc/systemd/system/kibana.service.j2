[Unit]
Description=Kibana 4

[Service]
Type=simple
User=kibana
Environment=CONFIG_PATH={{ kibana_dir }}/kibana-{{ kibana_version }}-linux-x64/config/kibana.yml
Environment=NODE_ENV=production
ExecStart={{ kibana_dir }}/kibana-{{ kibana_version }}-linux-x64/node/bin/node {{ kibana_dir }}/kibana-{{ kibana_version }}-linux-x64/src/bin/kibana.js

[Install]
WantedBy=multi-user.target
